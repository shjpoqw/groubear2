<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings><!-- value 에 NULL 은 무조건 대문자로  -->
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.kh.groubear.member.model.vo.MemberView" alias="MemberView"/>
		<typeAlias type="com.kh.groubear.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.groubear.member.model.vo.Job" alias="Job"/>
		<typeAlias type="com.kh.groubear.member.model.vo.Department" alias="Dept"/>


		<typeAlias type="com.kh.groubear.lmj.sns.model.vo.Sns" alias="Sns"/>
		<typeAlias type="com.kh.groubear.lmj.sns.model.vo.Attechment" alias="Attechment"/>
		<typeAlias type="com.kh.groubear.lmj.sns.model.vo.Reply" alias="Reply"/>

		<typeAlias type="com.kh.groubear.lyj.approval.model.vo.Approval" alias="Approval"/>
