		<typeAlias type="com.kh.groubear.kms.room.model.vo.ReserveRoom" alias="ReserveRoom"/>
	</typeAliases>


	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>


		<mapper resource="/mappers/sns-mapper.xml"/>
		<mapper resource="/mappers/snsAttechment-mapper.xml"/>
		<mapper resource="/mappers/snsReply-mapper.xml"/>

		<mapper resource="/mappers/approval-mapper.xml"/>
		<mapper resource="/mappers/contact-mapper.xml"/>
		<mapper resource="/mappers/messenger-mapper.xml"/>
		<mapper resource="/mappers/room-mapper.xml"/>
	</mappers>
	
	
</configuration>
